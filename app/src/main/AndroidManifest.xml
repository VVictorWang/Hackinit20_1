<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.viseator.hackinit20_1">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY"/>
    <uses-permission android:name="android.permission.BIND_NOTIFICATION_LISTENER_SERVICE"/>

    <application
        android:name=".App"
        android:allowBackup="true"
        android:largeHeap="true"
        android:icon="@mipmap/starticon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/starticon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.MonitorGameActivity">
        </activity>
        <activity android:name=".activity.SettingActivity"/>
        <activity android:name=".activity.RemindActivity">
        </activity>
        <activity android:name=".activity.ContactRecordActivity">
        </activity>
        <activity android:name=".activity.MonitorBehaviorActivity">
        </activity>
    </application>

</manifest>